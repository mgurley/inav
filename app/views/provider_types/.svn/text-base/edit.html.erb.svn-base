<h1>Editing provider_type</h1>

<% form_for(@provider_type) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @provider_type %> |
<%= link_to 'Back', provider_types_path %>
